<% if object.errors.any? %>
  <div class="alert alert-danger" role="alert">
    <% if object.errors.any? %>
      <div id="error_explanation">
        <ul>
          <% object.errors.full_message.each do |msg| %>
            <li>
            <%= msg %>
            </li>
          <% end %>
        </ul>
      </div>
    <% end %>
  </div>
<% end %>
